{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true,
        "codeceptjs/codeceptjs": true
    },
    "plugins": ["codeceptjs"],
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "import/no-extraneous-dependencies": 0,
        "no-return-assign": 0,
        "no-param-reassign": 0,
        "no-underscore-dangle": 0
    }
}
